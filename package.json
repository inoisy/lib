    "generate": "nuxt generate",
    "el:check": "eslint --ext .js --ext .vue .",
    "sl:check": "stylelint \"**/*.{scss,vue}\"",
    "lint:check": "npm run el:check && npm run sl:check",
    "el:fix": "eslint --ext .vue --ext .js . --fix",
    "sl:fix": "stylelint \"**/*.{scss,vue}\" --fix",
    "elsl:fix": "eslint --ext .vue --ext .js . --fix && stylelint \"**/*.{scss,vue}\" --fix",
    "lint:fix": "npm run el:fix && npm run sl:fix",
    "updateList": "ncu --reject sass-loader,sass,svgo",
    "update": "ncu -u --reject sass-loader,sass,svgo",
    "test": "jest",
    "test:output": "jest --outputFile=testResult.json --json",
    "test:update": "jest --updateSnapshot"
},
